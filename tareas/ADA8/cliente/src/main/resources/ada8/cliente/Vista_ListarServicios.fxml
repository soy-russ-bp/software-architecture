<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="cajaVerticalRegistros" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ada8.cliente.controller.ListarServiciosControlador">
  <padding>
    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
  </padding>
   <TextField fx:id="palabraInput" />
   <Button mnemonicParsing="false" onAction="#buscarServicios" text="Buscar" />
   <TableView fx:id="servicios" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="serviciosCol" prefWidth="199.0" text="Eventos" />
     </columns>
   </TableView>
</VBox>
