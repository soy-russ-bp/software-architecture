  <padding>
    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
  </padding>
  <VBox fx:id="productosVBox" alignment="CENTER" spacing="5" />
   <HBox fx:id="cajaHorizontalCabecera" prefHeight="100.0" prefWidth="200.0" />
   <TableView fx:id="eventos" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="eventosCol" prefWidth="199.0" text="Eventos" />
     </columns>
   </TableView>
</VBox>
